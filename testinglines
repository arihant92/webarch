  <canvas id="svg" width="300" height="300"></canvas>

<script>
var document= d3.select("svg");
var draw= document.append("line").attr("x1",10).attr("x2",10).attr("y1",60).attr("y2",80).attr("stroke-width",5)
                                  .attr("stroke",red);
  
</script>
